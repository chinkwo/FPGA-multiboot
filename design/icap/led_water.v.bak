module	led_water(
		input			wire			sclk		,
		input			wire			rst_n		,
		input			wire			key_in	,
		output		wire[3:0]	led   	

);
wire		key_flag	;

//key_debounceÀı»¯
key_debounce		U0(
.rst_n		(rst_n		),
.sclk			(sclk			),
.key			(key_in		),
