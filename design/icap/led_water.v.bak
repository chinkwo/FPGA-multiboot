);

icap_ctrl	U1(
.sclk			(sclk			),
.rst_n		(rst_n		),
.key_flag (key_flag )		
);

water_led	U2(
.sclk		(sclk	),
.rst_n	(rst_n),
.led    (led  )
);

endmodule		